<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/privacy"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF9E3"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="410dp"
        android:layout_height="650dp"
        android:layout_marginTop="110dp"
        android:background="@drawable/shape_profile" />

    <ImageButton
        android:id="@+id/backbuttonnotif"
        android:layout_width="44dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/bg_newreport"
        app:srcCompat="@drawable/ic_baseline_arrow_back_ios_new_24" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/montserrat_semibold"
        android:text="Settings"
        android:textColor="#17395C"
        android:textSize="34sp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="675dp"
        android:background="@drawable/bottom_nav_student"
        android:elevation="2dp"
        app:itemIconSize="28dp"
        app:itemIconTint="@color/bottom_item_backgroundcolor"
        app:itemTextColor="@color/bottom_item_backgroundcolor"
        app:menu="@menu/bottom_nav_student">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

    <Button
        android:id="@+id/privacysettings"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="180dp"
        android:backgroundTint="#E1DCC9"
        android:text="Privacy" />

    <Button
        android:id="@+id/aboutussettings"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="300dp"
        android:backgroundTint="#E1DCC9"
        android:text="About Us" />


</androidx.coordinatorlayout.widget.CoordinatorLayout>
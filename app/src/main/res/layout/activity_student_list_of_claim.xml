<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_newreport"
    tools:context=".Student_ListOfClaim">


    <TextView
        android:id="@+id/rectanglehide"
        android:layout_width="match_parent"
        android:layout_height="85dp"
        android:layout_marginTop="10dp"
        android:background="#FFF9E3"
        />


    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="625dp"
        android:layout_gravity="center"
        android:fontFamily="@font/montserrat_semibold"
        android:text="List Of Claim"
        android:textColor="#17395C"
        android:textSize="34sp" />

    <androidx.recyclerview.widget.RecyclerView

        android:id="@+id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="575dp"
        android:layout_marginTop="110dp"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="730dp"
        android:background="@drawable/bottom_nav_student"

        app:itemIconSize="25dp"
        app:itemIconTint="@color/bottom_item_backgroundcolor"
        app:itemTextColor="@color/bottom_item_backgroundcolor"
        app:menu="@menu/bottom_nav_student">

    </com.google.android.material.bottomnavigation.BottomNavigationView>




</androidx.coordinatorlayout.widget.CoordinatorLayout>